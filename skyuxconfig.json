{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "skyux",
  "mode": "easy",
  "compileMode": "aot",
  "host": {
    "url": "https://developer.blackbaud.com"
  },
  "app": {
    "title": "Blackbaud SKY UX",
    "styles": [
      "@skyux/theme/css/sky.css",
      "@skyux/docs-tools/css/docs-tools.css",
      "@skyux/ag-grid/css/sky-ag-grid.css"
    ]
  },
  "appSettings": {
    "stache": {
      "editButton": {
        "text": "Suggest edit",
        "url": "https://github.com/blackbaud/skyux2-docs"
      },
      "footer": {},
      "searchConfig": {
        "is_internal": false,
        "site_names": [
          "skyux",
          "bb-help-docs",
          "skyux-migration-guide"
        ]
      }
    }
  },
  "omnibar": {
    "experimental": true
  },
  "params": {
    "svcid": {
      "value": "skyux"
    }
  },
  "routes": {
    "public": [
      {
        "title": "Home",
        "route": "/",
        "global": true
      },
      {
        "title": "Learn",
        "route": "/learn",
        "global": true
      },
      {
        "title": "Components",
        "route": "/components",
        "global": true
      },
      {
        "title": "Design",
        "route": "/design",
        "global": true
      },
      {
        "title": "Contribute",
        "route": "/contribute",
        "global": true
      },
      {
        "title": "Blog",
        "route": "/blog",
        "global": true
      }
    ]
  },
  "plugins": [
    "@blackbaud/skyux-builder-plugin-stache",
    "@blackbaud/skyux-builder-plugin-code-block"
  ],
  "redirects": {
    "learn/get-started/1.-SSL-certification": "learn/get-started/prereqs/ssl-certificate",
    "learn/get-started/2.-initial-setup": "learn/get-started/prereqs/initial-setup",
    "learn/get-started/3.-create-project": "learn/get-started/basics/create-project",
    "learn/get-started/4.-serve-project": "learn/get-started/basics/serve-project",
    "learn/get-started/5.-edit-project": "learn/get-started/basics/edit-project",
    "learn/get-started/6.-add-entry-component": "learn/get-started/advanced/add-entry-components",
    "learn/get-started/7.-run-tests": "learn/get-started/basics/run-tests",
    "learn/get-started/8.-unit-test-modals": "learn/get-started/advanced/unit-test-modals",
    "learn/get-started/9.-include-static-asset": "learn/get-started/advanced/include-static-assets",
    "design/accessibility": "learn/accessibility",
    "learn/accessibility/code-review": "learn/accessibility/test",
    "design/accessibility/code-review": "learn/accessibility/test",
    "learn/accessibility/keyboard-testing": "learn/accessibility/test",
    "design/accessibility/keyboard-testing": "learn/accessibility/test",
    "learn/faq": "learn/reference/faq",
    "learn/resources": "learn/reference/resources",
    "learn/get-started/add-entry-components": "learn/get-started/advanced/add-entry-components",
    "learn/get-started/create-project": "learn/get-started/basics/create-project",
    "learn/get-started/serve-project": "learn/get-started/basics/serve-project",
    "learn/get-started/edit-project": "learn/get-started/basics/edit-project",
    "learn/get-started/include-static-assets": "learn/get-started/advanced/include-static-assets",
    "learn/get-started/initial-setup": "learn/get-started/prereqs/initial-setup",
    "learn/get-started/run-tests": "learn/get-started/basics/run-tests",
    "learn/get-started/ssl-certificate": "learn/get-started/prereqs/ssl-certificate",
    "learn/get-started/unit-test-modals": "learn/get-started/advanced/unit-test-modals",
    "learn/contribute": "contribute",
    "learn/contribute/accessibility": "design/accessibility",
    "learn/contribute/accessibility/code-review": "design/accessibility/code-review",
    "learn/contribute/accessibility/keyboard-testing": "design/accessibility/keyboard-testing",
    "components/tiles": "components/tile",
    "design/borders": "design/styles/borders",
    "design/color": "design/styles/color",
    "design/icons": "design/styles/icons",
    "design/styles/icons": "components/icon",
    "design/motion": "design/styles/motion",
    "design/spacing": "design/styles/spacing",
    "design/typography": "design/styles/typography",
    "design/guidelines/form-containers": "design/guidelines/form-design",
    "design/guidelines/select-items": "design/guidelines/form-design",
    "components/list-filters": "components/list/filters",
    "components/list-paging": "components/list/paging",
    "components/list-toolbar": "components/list/toolbar",
    "components/list-view-checklist": "components/list/checklist",
    "components/list-view-grid": "components/list/grid",
    "learn/get-started/basics/update-components": "learn/get-started/basics/modules",
    "learn/get-started/advanced/add-entry-components": "learn/get-started/advanced/entry-components",
    "learn/reference/helpers/SkyAuthHttpClientModule": "learn/reference/helpers",
    "learn/reference/faq": "learn/reference",
    "contribute/guidelines": "contribute/contribution-process",
    "components/fileattachments": "components/file-attachments",
    "learn/get-started/prereqs/ssl-certificate": "learn/get-started/prereqs",
    "learn/get-started/prereqs/initial-setup": "learn/get-started/prereqs",
    "design/resources": "design"
  }
}
